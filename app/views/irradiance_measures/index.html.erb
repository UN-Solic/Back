<p id="notice"><%= notice %></p>

<h1>Irradiance Measures</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @irradiance_measures.each do |irradiance_measure| %>
      <tr>
        <td><%= link_to 'Show', irradiance_measure %></td>
        <td><%= link_to 'Edit', edit_irradiance_measure_path(irradiance_measure) %></td>
        <td><%= link_to 'Destroy', irradiance_measure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Irradiance Measure', new_irradiance_measure_path %>
